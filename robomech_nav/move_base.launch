<launch>

	<arg name="static_map" default="false"/>
	<arg name="base_global_planner" default="navfn/NavfnROS"/>
	<arg name="base_local_planner" default="dwa_local_planner/DWAPlannerROS"/>

<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<param name="base_global_planner" value="$(arg base_global_planner)"/>
	<param name="base_local_planner" value="$(arg base_local_planner)"/> 
	<param name="controller_frequency" value="10.0"/>  

	<rosparam file="$(find robomech_nav)/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
	<rosparam file="$(find robomech_nav)/costmap_common_params.yaml" command="load" ns="local_costmap" />
	<rosparam file="$(find robomech_nav)/local_costmap_params.yaml" command="load" />
	<rosparam file="$(find robomech_nav)/global_costmap_params.yaml" command="load" /> 
	<rosparam file="$(find robomech_nav)/base_local_planner_params.yaml" command="load" />
</node>

</launch> 
