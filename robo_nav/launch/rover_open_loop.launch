<launch>
    <arg name="rover_closed_loop_controller" default="false"/>

    <!-- OpenRover Driver -->
    <node pkg="rr_openrover_driver" type="openrover_driver_node" name="rr_openrover_driver_open_loop" respawn="false" output="screen">
        <param name="port" value="/dev/rover-pro" />
        <!-- <param name="drive_type" value="2wd" /> -->
        <param name="drive_type" value="4wd" />
        <param name="enable_timeout" type="bool" value="true"/>
        <param name="timeout" type="double" value="0.3"/>
        <param name="closed_loop_control_on" type="bool" value="$(arg rover_closed_loop_controller)" />
        <!--<param name="closed_loop_control_on" type="bool" value="true" />--><!-- Requires fast_data_rate >= 60 -->
        <param name="total_weight" type="double" value="24.4"/>
        <!-- <param name="traction_factor" value="0.610"/> -->
        <param name="traction_factor" value="0.33"/>
        <param name="odom_covariance_0" value="0.01"/>
        <param name="odom_covariance_35" value="0.03"/>

        <param name="fast_data_rate" value="30.0"/>
        <param name="medium_data_rate" value="2.0"/>
        <param name="slow_data_rate" value="1.0"/>
    </node>

</launch>